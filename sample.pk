module sample;

type point {
    int x;
    int y;
}

type line {
    point p1;
    point p2;
    bool direction;
}

int bad_fib(int index) {
    if (index < 2) {
        return 1;
    } else {
        return bad_fib(index - 1) + bad_fib(index - 2);
    }
}

int num;

bad_fib(num);
bad_fib(1);
bad_fib(2);
bad_fib(3);
bad_fib(4);
bad_fib(5);
bad_fib(6);
bad_fib(7);
bad_fib(8);

point p;
line l;

bad_fib(20);